{
  "devDependencies": {
    "@schibstedspain/frontend-pre-commit-rules": "^8.1.1",
    "eslint": "^9.12.0",
    "scss-lint": "^0.0.0"
  },
  "scripts": {
    "lint:js": "linting-rules js",
    "lint": "npm run lint:js && npm run lint:sass",
    "lint:eslint": "eslint --ext=.jsx --ext=.js ./src/",
    "lint:sass": "scss-lint ./src/"
  },
  "pre-commit": [
    "lint:eslint"
  ]
}
